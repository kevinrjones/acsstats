<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"] - AcsStatsWeb</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" media="screen"/>
    <link rel="stylesheet" href="~/lib/fontawesome/css/all.css" media="screen"/>
    <link rel="stylesheet" href="~/css/site.css" media="screen"/>

</head>
<body>

<div class="container">
    <main role="main" class="pb-3">
        <div class="scorecard-container">
            <div class="sidebar">

            </div>
            <div class="scorecard-main">
                @RenderBody()
            </div>
            <div class="sidebar">

            </div>

        </div>
    </main>
</div>

<footer class="border-top footer text-muted">
    <div class="container">
        &copy; 2022 - Kevin Jones - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
    </div>
</footer>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
<script src="~/js/app.js"></script>
@await RenderSectionAsync("Scripts", required: false)
</body>
</html>